<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
<!--
	<properties resource="conn.properties" />
	  -->
	  <!-- 处理1 -->
	 <classPathEntry location="D:/STD/java/myclasses/mysqlDriver/mysql-connector-java-5.1.7/mysql-connector-java-5.1.7-bin.jar"/>
	<!-- 指定运行环境是mybatis3的版本 -->
	<context id="testTables" targetRuntime="MyBatis3">

		<commentGenerator>
		<!-- 是否取消注释 -->
			<property name="suppressAllComments" value="true" />
			 <!-- 是否生成注释代时间戳 -->
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- 处理2   jdbc 连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/up_vams?useUnicode=true&amp;characterEncoding=UTF-8" userId="root" password="root">
		</jdbcConnection>
		
        <!--处理3 
          targetPackage指定模型在生成在哪个包 ,targetProject指定项目的src,-->        
		<javaModelGenerator targetPackage="com.up_vams.user.entity"
			targetProject="up_vams/src">
			<!-- 去除字段前后空格 -->
			<property name="trimStrings" value="false" />
		</javaModelGenerator>
		<!--处理4   配置SQL映射文件生成信息 -->
		<sqlMapGenerator targetPackage="com.up_vams.user.dao"
			targetProject="up_vams/src" />
		<!-- 处理5   配置dao接口生成信息-->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.up_vams.user.dao" targetProject="up_vams/src" />
<!-- 
		<table tableName="account" domainObjectName="Account"/>			
		<table tableName="supplier" domainObjectName="Supplier"/> 
		<table tableName="goods" domainObjectName="Goods"/>		
		<table tableName="account_records" domainObjectName="AccountRecords"/>			
		<table tableName="buy_order" domainObjectName="BuyOrder"/> 
		<table tableName="buy_order_detail" domainObjectName="BuyOrderDetail"/>
		-->
		<table tableName="t_user" domainObjectName="User"/>
		

		
	</context>
</generatorConfiguration>